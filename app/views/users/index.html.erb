<div class="center">
  <%= will_paginate %>
</div>
<% @users.each do |user| %>
  <ul class="listing">
    <div class="row">
      <div class="col-md-4 col-md-offset-4">
        <div class="well well-lg center">
          <li>
            <%= link_to gravatar_for(user), user_path(user)  %>
            <h4><%= link_to user.username,user_path(user) %></h4>
            <small><%= pluralize(user.articles.count, "article")%></small>
          </li>
        </div>
      </div>
    </div>
  </ul>
<% end %>
<div class="center">
  <%= will_paginate %>
</div>